<form action="/dealerships" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new dealership:</p>
  <p> name: </p>
  <input type='text' name='dealership[name]'/><br/>
  <p> zip: </p>
  <input type='text' name='dealership[zip]'/><br/>
  <p> operational: </p>
  <input type="radio" name="dealership[operational]" id='dealership_operational_true' value='true'> YES </input>
  <input type="radio" name="dealership[operational]" id='dealership_operational_false' value='false'> NO </input>
  <input type='submit' value="Submit"/>
</form>

